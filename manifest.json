{
  "manifest_version": 3,
  "name": "YouTube Ad Skipper",
  "version": "1.0",
  "description": "Automatically skips or minimizes YouTube ads.",
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
